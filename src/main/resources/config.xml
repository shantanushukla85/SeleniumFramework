<?xml version="1.0" encoding="UTF-8"?>
<extentreports>
	<configuration>
		<!-- document encoding -->
		<encoding>UTF-8</encoding>

		<!-- title of the document -->
		<documentTitle>ASC_Automation</documentTitle>

		<!-- report name - displayed at top-nav -->
		<reportName>
            <![CDATA[
                <img src="https://www.stryker.com/etc/designs/stryker/images/header/logo.png"/ align="left">
               &nbsp; &nbsp; ASC_Automation
            ]]>
			
		</reportName>

		<!-- report headline - displayed at top-nav, after reportHeadline -->
		<reportHeadline></reportHeadline>

		<!-- custom javascript -->
		<scripts>
        <![CDATA[
        $(document).ready(function() {
        $('.logo-container.blue.darken-2').css('display','none');
        });
        ]]>
		</scripts>

		<!-- custom styles -->
		<styles>
            <![CDATA[
                .report-name { padding-left: 5px; } .report-name > img { float: left;height: 90%;margin-left: -80px;margin-top: 1.5px;width: auto; }
            ]]>
		</styles>
	</configuration>
</extentreports>